SKIP: { " " | "\r" | "\n" | "\t" }

TOKEN:
{
  <DIGIT_DEC: ["0"-"9"]>
| <DIGIT_HEX: ["0"-"9", "a"-"f", "A"-"F"]>
}

long number():
{
  long sum = 0;
  Token t = null;
}
{
  (t = <DIGIT_DEC>)*
  {
    return Long.parseLong(t.toString());
  }
}
